{
  "name": "is-invitelink",
  "productName": "Dangerous Users",
  "version": "1.0.0",
  "description": "Check all Dangerous Users and report them!",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "produce": "electron-builder"
  },
  "build": {
    "appId": "Dangerous-Users",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/installericon.ico"
    },
    "nsis": {
      "installerIcon": "assets/installericon.ico",
      "uninstallerIcon": "assets/installericon.ico",
      "uninstallDisplayName": "Dangerous Users",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "tarajohns0n",
    "email": "johnson.tara5@student.oaklandcc.edu"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "is_invitelink"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "12.0.2",
    "electron-builder": "^22.10.5"
  }
}
